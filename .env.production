# QuantConnect Trading Bot - Production Environment Configuration
# Generated for eqtrader.ddnskita.my.id
# Copy to .env and configure with your actual credentials

# ============================================================================
# ENVIRONMENT SETTINGS
# ============================================================================
NODE_ENV=production
ENVIRONMENT=production
DEBUG=false
FLASK_ENV=production
FLASK_DEBUG=false

# ============================================================================
# DOMAIN AND DDNS CONFIGURATION  
# ============================================================================
DOMAIN=eqtrader.ddnskita.my.id
DDNS_UPDATE_URL=https://tunnel.hostddns.us/ddns/377b9a29c7bba5435e4b5d53e3ead4aa
DDNS_CHECK_INTERVAL=300
DDNS_UPDATE_INTERVAL=300
AUTO_UPDATE_DDNS=true
DDNS_TIMEOUT=30
DDNS_RETRY_ATTEMPTS=3
DDNS_RETRY_DELAY=60

# ============================================================================
# SSL CONFIGURATION
# ============================================================================
SSL_ENABLED=true
SSL_CERT_PATH=/etc/letsencrypt/live/eqtrader.ddnskita.my.id/fullchain.pem
SSL_KEY_PATH=/etc/letsencrypt/live/eqtrader.ddnskita.my.id/privkey.pem
SSL_CHAIN_PATH=/etc/letsencrypt/live/eqtrader.ddnskita.my.id/chain.pem

# ============================================================================
# APPLICATION URLs
# ============================================================================
API_BASE_URL=https://eqtrader.ddnskita.my.id/api
FRONTEND_URL=https://eqtrader.ddnskita.my.id
WEBSOCKET_URL=wss://eqtrader.ddnskita.my.id/socket.io

# React App URLs (for build time)
REACT_APP_API_URL=https://eqtrader.ddnskita.my.id/api
REACT_APP_WS_URL=wss://eqtrader.ddnskita.my.id/socket.io
REACT_APP_DOMAIN=eqtrader.ddnskita.my.id

# ============================================================================
# DATABASE CONFIGURATION
# ============================================================================
POSTGRES_DB=trading_bot
POSTGRES_USER=postgres
POSTGRES_PASSWORD=CHANGE_THIS_SECURE_PASSWORD_123!
DATABASE_URL=postgresql://postgres:CHANGE_THIS_SECURE_PASSWORD_123!@db:5432/trading_bot

# Database connection pool settings
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_ECHO=false

# ============================================================================
# REDIS CONFIGURATION
# ============================================================================
REDIS_PASSWORD=CHANGE_THIS_REDIS_PASSWORD_456!
REDIS_URL=redis://:CHANGE_THIS_REDIS_PASSWORD_456!@redis:6379/0

# ============================================================================
# FLASK SECURITY CONFIGURATION
# ============================================================================
SECRET_KEY=CHANGE_THIS_FLASK_SECRET_KEY_789!
JWT_SECRET_KEY=CHANGE_THIS_JWT_SECRET_KEY_ABC!
JWT_ACCESS_TOKEN_EXPIRES=1800
JWT_REFRESH_TOKEN_EXPIRES=2592000
BCRYPT_LOG_ROUNDS=12

# ============================================================================
# CORS AND SECURITY
# ============================================================================
CORS_ORIGINS=https://eqtrader.ddnskita.my.id
SOCKETIO_CORS_ORIGINS=https://eqtrader.ddnskita.my.id
TRUSTED_HOSTS=eqtrader.ddnskita.my.id,localhost
RATE_LIMIT_PER_MINUTE=60
ENABLE_CORS=true
MAX_CONTENT_LENGTH=16777216

# ============================================================================
# QUANTCONNECT LEAN CONFIGURATION
# ============================================================================
QC_USER_ID=YOUR_QUANTCONNECT_USER_ID
QC_API_TOKEN=YOUR_QUANTCONNECT_API_TOKEN
QC_ENVIRONMENT=live-trading
QC_DATA_FOLDER=/app/lean/data
QC_ALGORITHM_FOLDER=/app/lean/algorithms
QC_RESULTS_FOLDER=/app/lean/results

# ============================================================================
# MACHINE LEARNING CONFIGURATION
# ============================================================================
MODELS_DIR=/app/models/trained_models
CONFIGS_DIR=/app/models/model_configs
MAX_MODEL_SIZE=104857600
ML_DEFAULT_TEST_SIZE=0.2
ML_DEFAULT_RANDOM_STATE=42
ML_FEATURE_IMPORTANCE_THRESHOLD=0.01

# ============================================================================
# TRADING CONFIGURATION
# ============================================================================
MAX_DAILY_LOSS=0.05
MAX_POSITION_SIZE=0.10
MAX_CORRELATION=0.70
DEFAULT_LEVERAGE=1
SIGNAL_CONFIDENCE_THRESHOLD=0.60
MAX_CONCURRENT_TRADES=10
TRADE_TIMEOUT_SECONDS=300

# ============================================================================
# LOGGING CONFIGURATION
# ============================================================================
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE_PATH=/app/logs/trading_bot.log
LOG_MAX_BYTES=10485760
LOG_BACKUP_COUNT=5
ENABLE_JSON_LOGGING=false

# ============================================================================
# MONITORING AND ALERTING
# ============================================================================
ENABLE_HEALTH_CHECK=true
HEALTH_CHECK_INTERVAL=30
METRICS_RETENTION_DAYS=30
ALERT_EMAIL=your-email@example.com
SLACK_WEBHOOK=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN
TELEGRAM_CHAT_ID=YOUR_TELEGRAM_CHAT_ID

# ============================================================================
# DATA DIRECTORIES
# ============================================================================
DATA_DIR=/opt/quantconnect-trading-bot/data
LOGS_DIR=/opt/quantconnect-trading-bot/logs
BACKUP_DIR=/opt/quantconnect-trading-bot/backups

